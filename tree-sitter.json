{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "prism",
      "camelcase": "prism",
      "title": "prism",
      "scope": "source.prism",
      "file-types": [
        "prism"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^prism$",
      "class-name": "TreeSitterPrism"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter grammar for the PRISM-games project, focused on CSGs",
    "authors": [
      {
        "name": "BrandonTang89",
        "email": "tangyuhanbrandon@gmail.com",
        "url": "https://brandontang89.github.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-prism"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
